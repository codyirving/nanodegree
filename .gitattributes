/build export-ignore  
.gitattributes export-ignore  
.gitignore export-ignore

# built application files
*.apk export-ignore
*.ap_ export-ignore

# files for the dex VM
*.dex export-ignore

# Java class files
*.class export-ignore

# generated files
bin/ export-ignore
gen/ export-ignore

# Local configuration file (sdk path, etc)
local.properties export-ignore

# Eclipse project files
.classpath export-ignore
.project export-ignore

# Proguard folder generated by Eclipse
proguard/ export-ignore

# Intellij project files
*.iws export-ignore
.idea/workspace.xml export-ignore
.idea/tasks.xml export-ignore